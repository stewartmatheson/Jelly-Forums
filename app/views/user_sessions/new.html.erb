
<% title "Log in" %>  
  
<% form_for @user_session do |form| %>  
<%= form.error_messages %>  
  
  <p>  
    <%= form.label :email, "Email Address" %><br /> 
    <%= form.text_field :email %>  
  </p>  
  <p>  
    <%= form.label :password, "Password" %><br />
    <%= form.password_field :password %>  
  </p>  
  <p>  
    <%= form.submit "Submit" %>  
  </p>  
  
<% end %> 
